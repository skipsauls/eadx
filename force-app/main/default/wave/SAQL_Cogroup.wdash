 We want just the 2017 data and the won opportunities \\nq = filter q by date('Close_Date_Year', 'Close_Date_Month', 'Close_Date_Day') in [dateRange([2017,1,1], [2017,12,31])]; \\nq = filter q by 'Stage' == \\\"08 - Closed Won\\\";\\n q = group q by 'Account_Owner'; \\n\\n -- project the sum of amount for each owner \\nq = foreach q generate 'Account_Owner' as 'Account_Owner', sum('Amount') as 'sum_Amount'; \\n\\n -- sort the results alphabetically by owner \\n q = order q by 'Account_Owner' asc; q = limit q 2000;\",\"chart\":\"vbar\",\"reflinelabel\":\"\",\"reflinecolor\":\"rgb(225, 243, 251)\",\"reflinevalue\":0}","{\"display\":\"Percent Attained\",\"value\":\"-- load the opportunity dataset \\nopt = load \\\"eadx__Wave_Sample_Opportunities\\\"; \\n\\n -- we just want 2017 data and won opportunities \\n opt = filter opt by date('Close_Date_Year', 'Close_Date_Month', 'Close_Date_Day') in [dateRange([2017,1,1], [2017,12,31])]; \\nopt = filter opt by 'Stage' == \\\"08 - Closed Won\\\"; \\n\\n -- load the quota dataset \\n quo = load \\\"eadx__Quota_Region1\\\"; \\n\\n -- we just want 2017 data \\n quo = filter quo by 'Year' == \\\"2017\\\"; \\n\\n -- cogroup the two datasets \\natt = group opt by 'Account_Owner', quo by 'Employee'; \\n\\n -- calculate the percent attained for each owner \\n att = foreach att generate opt.'Account_Owner' as 'Owner', sum(opt.'Amount')/sum(quo.'Quota') as 'percent attained';\",\"chart\":\"hbar\",\"reflinelabel\":\"100%\",\"reflinecolor\":\"rgb(9, 26, 62)\",\"reflinevalue\":1}"]},"all_2":{"broadcastFacet":true,"datasets":[{"name":"Quota_Region1","namespace":"eadx"}],"isGlobal":false,"query":{"query":"{\"measures\":[[\"count\",\"*\"]]}","version":-1.0},"receiveFacetSource":{"mode":"all","steps":[]},"selectMode":"single","type":"aggregateflex","useGlobal":true,"visualizationParameters":{"parameters":{"autoFitMode":"keepLabels","showValues":true,"bins":{"breakpoints":{"high":100,"low":0},"bands":{"high":{"color":"#008000","label":""},"low":